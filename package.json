{
  "name": "traque-backend",
  "version": "0.1.0",
  "description": "Traque Backend",
  "author": "Speekl, LLC",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/app",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/app",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --detectOpenHandles --coverage",
    "typeorm:migration:generate": "npm run build && echo 'Enter migration name: ' && read name && typeorm migration:generate -d dist/core/db/config.js src/core/db/migrations/$name",
    "typeorm:migration:create": "npm run build && echo 'Enter migration name: ' && read name && typeorm migration:create src/core/db/migrations/$name",
    "better-auth:migration:generate": "npx @better-auth/cli generate --config ./src/core/auth/auth.ts"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^1.0.15",
    "@ai-sdk/openai": "^2.0.23",
    "@better-auth/expo": "^1.3.9",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.5",
    "@nestjs/core": "^11.0.5",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@polar-sh/better-auth": "^1.1.3",
    "@polar-sh/sdk": "^0.34.16",
    "@scalar/nestjs-api-reference": "^0.5.9",
    "ai": "^5.0.28",
    "axios": "^1.8.2",
    "better-auth": "^1.3.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^3.1.0",
    "dayjs": "^1.11.13",
    "expo-server-sdk": "^3.13.0",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nestjs-typeorm-paginate": "^4.0.4",
    "pg": "^8.13.1",
    "query-string": "^9.1.1",
    "reflect-metadata": "^0.2.2",
    "remeda": "^2.30.0",
    "resend": "^6.0.1",
    "rxjs": "^7.8.1",
    "ts-pattern": "^5.7.0",
    "typeorm": "^0.3.20",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
